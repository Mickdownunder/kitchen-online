{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "portal.kuechenonline.com"
        }
      ],
      "destination": "/portal"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "portal.kuechenonline.com"
        }
      ],
      "destination": "/portal/:path*"
    }
  ]
}
